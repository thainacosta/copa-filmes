@model (Filme Campeao, Filme ViceCampeao)
@{
    ViewData["Title"] = "Resultado do Campeonato";
    ViewData["Descricao"] = "Veja o resultado final do Campeonato de Filmes de forma simples e rápida.";
    ViewData["Titulo"] = "Resultado Final";
}

<div class="row ml-0 mt-3 mr-0">
    <div class="col-md-1 bg-secondary text-white display-4 pt-3 pb-3 text-center">
        1º
    </div>
    <div class="col-md-11 bg-white mb-0">
        <p class="text-secondary h5 text-secondary h5 mt-4 pt-3">@Model.Campeao.Titulo</p>
    </div>
</div>
<div class="row ml-0 mt-3 mr-0">
    <div class="col-md-1 bg-secondary text-white display-4 pt-3 pb-3 text-center">
        2º
    </div>
    <div class="col-md-11 bg-white text-secondary">
        <p class="text-secondary h5 text-secondary h5 mt-4 pt-3">@Model.ViceCampeao.Titulo</p>
    </div>
</div>
